{
  "name": "superloop-ui",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "bin": {
    "superloop-ui": "dist/cli.js"
  },
  "scripts": {
    "build:css": "bunx postcss src/styles/globals.css -o dist/web/styles.css",
    "build:js": "bunx tsup",
    "build": "bun install && bun run build:css && bun run build:js && node dist/build-assets.js",
    "dev": "bun install && bun run src/cli.ts dev",
    "postinstall": "node scripts/ensure-biome-bin.cjs",
    "start": "node dist/cli.js",
    "typecheck": "bun install && bunx tsc --noEmit",
    "lint": "bunx biome check .",
    "format": "bunx biome format --write .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@biomejs/biome": "^1.9.4",
    "@inquirer/prompts": "^7.2.0",
    "@json-render/core": "file:../json-render-core",
    "@json-render/react": "file:../json-render-react",
    "@radix-ui/react-accordion": "^1.2.0",
    "@radix-ui/react-checkbox": "^1.1.0",
    "@radix-ui/react-collapsible": "^1.1.0",
    "@radix-ui/react-dialog": "^1.1.0",
    "@radix-ui/react-dropdown-menu": "^2.1.0",
    "@radix-ui/react-scroll-area": "^1.2.0",
    "@radix-ui/react-select": "^2.1.0",
    "@radix-ui/react-slider": "^1.2.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.0",
    "@radix-ui/react-tooltip": "^1.1.0",
    "ansi-to-react": "^6.1.0",
    "shiki": "^1.0.0",
    "blessed": "^0.1.81",
    "chalk": "^5.3.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "commander": "^12.1.0",
    "framer-motion": "latest",
    "lucide-react": "^0.400.0",
    "ora": "^8.1.1",
    "react": "latest",
    "react-dom": "latest",
    "tailwind-merge": "^2.2.0",
    "zod": "^4.0.0"
  },
  "devDependencies": {
    "@types/blessed": "^0.1.25",
    "@types/node": "^20.16.1",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.0",
    "postcss-cli": "^11.0.0",
    "tailwindcss": "^3.4.0",
    "tsup": "^8.2.4",
    "typescript": "^5.6.0",
    "vitest": "^1.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "@vitest/ui": "^1.0.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/jest-dom": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "jsdom": "^23.0.0"
  }
}
