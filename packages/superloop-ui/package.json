{
  "name": "superloop-ui",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "bin": {
    "superloop-ui": "dist/cli.js"
  },
  "scripts": {
    "build": "bun install && bunx tsup && node dist/build-assets.js",
    "dev": "bun install && bun run src/cli.ts dev",
    "postinstall": "node scripts/ensure-biome-bin.cjs",
    "start": "node dist/cli.js",
    "typecheck": "bun install && bunx tsc --noEmit",
    "lint": "bunx biome check .",
    "format": "bunx biome format --write ."
  },
  "dependencies": {
    "@biomejs/biome": "^1.9.4",
    "@inquirer/prompts": "^7.2.0",
    "@json-render/core": "file:../json-render-core",
    "@json-render/react": "file:../json-render-react",
    "blessed": "^0.1.81",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "framer-motion": "latest",
    "ora": "^8.1.1",
    "react": "latest",
    "react-dom": "latest",
    "zod": "^4.0.0"
  },
  "devDependencies": {
    "@types/blessed": "^0.1.25",
    "@types/node": "^20.16.1",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "tsup": "^8.2.4",
    "typescript": "^5.6.0"
  }
}
