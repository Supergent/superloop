if [ "${USE_DEVENV:-1}" = "1" ]; then
  if command -v devenv >/dev/null 2>&1; then
    eval "$(devenv direnvrc)"
    use devenv
  fi
fi

dotenv_if_exists .env.local

export PORTLESS="${PORTLESS:-1}"
export SUPERGENT_BASE_URL="${SUPERGENT_BASE_URL:-http://supergent.localhost:1355}"
export SUPERGENT_LAB_BASE_URL="${SUPERGENT_LAB_BASE_URL:-http://lab.supergent.localhost:1355}"
